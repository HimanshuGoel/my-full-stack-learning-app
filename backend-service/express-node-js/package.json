{
  "name": "express-node-js",
  "version": "0.0.0",
  "scripts": {
    "start": "npx ts-node-dev --respawn --transpile-only ./src/server.ts",
    "start-live": "npx ts-node-dev --respawn --transpile-only ./src/live-reload-server.ts",
    "tsc": "tsc",
    "build": "tsc",
    "prod": "npm run verify && NODE_ENV=production && node ./build/src/server.js",
    "verify": "npm run lint && npm audit",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint --fix src/**/*.ts",
    "pretty": "prettier --write src/**/*.ts"
  },
  "dependencies": {
    "adm-zip": "^0.5.10",
    "body-parser": "^1.20.2",
    "chalk": "^4.1.2",
    "cors": "^2.8.5",
    "dotenv": "^16.1.4",
    "express": "^4.18.2",
    "glob": "10.2.6",
    "helmet": "^7.0.0",
    "http-status-codes": "^2.2.0",
    "mongodb": "^5.6.0",
    "morgan": "^1.10.0",
    "winston": "^3.9.0"
  },
  "devDependencies": {
    "@types/adm-zip": "^0.5.0",
    "@types/body-parser": "^1.19.2",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/glob": "^8.1.0",
    "@types/morgan": "^1.9.4",
    "@types/node": "^20.2.5",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "prettier": "^2.8.8",
    "ts-node-dev": "^2.0.0",
    "typescript": "~5.0.4"
  }
}
